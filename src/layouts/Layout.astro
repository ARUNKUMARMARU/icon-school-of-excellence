---
import { string } from 'astro:schema';

interface Props {
  title: string;
  description: string;
  keywords: string;
  og_title: string;
  og_description: string;
  og_image: string;
  og_url: string;
  canonical: string;
}

const {
  title,
  description,
  keywords,
  og_title,
  og_description,
  og_image,
  og_url,
  canonical,
} = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
