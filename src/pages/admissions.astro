---
import Layout from '../layouts/Layout.astro';
import Header from '../components/header.astro';
import Footer from '../components/footer.astro';
---

<Layout
  title="Icon School Of Excellence"
  description=""
  keywords=""
  og_title=""
  og_description=""
  og_image=""
  og_url=""
  canonical=""
>
  <body>
    <div class="page-wrapper">
      <Header />
      <main class="main-wrapper">
        <header class="section_about-us_header-section-vision">
          <div class="w-layout-grid about-us_header-section-vision_component">
            <div class="about-us_header-section-vision_image-wrapper">
              <img
                src="/assets/images/ICON-School-Pic.webp"
                loading="eager"
                sizes="(max-width: 991px) 100vw, 50vw"
                alt="icon-school-of-excellence-image"
                class="about-us_header-section-vision_image"
              />
            </div>
            <div
              blocks-name="header37_content"
              blocks-slot-children="ST265"
              class="about-us_header-section-vision_content"
            >
              <div>
                <h1 blocks-name="heading" class="heading-style-h1">
                  <strong>Admission</strong>
                </h1>
              </div>
              <p
                blocks-name="paragraph"
                class="text-size-medium text-color-primary"
              >
                ICON SCHOOL OF EXCELLENCE is dedicated to providing a
                high-quality education that fosters academic excellence and
                personal growth. We are excited to announce that classes for the
                upcoming academic year will commence in the first week of June.
                To learn more about our programs, curriculum, and admission
                process, please visit us at #118/2, E-City Phase I,
                Doddathoguru, Bangalore – 560100. For any inquiries, feel free
                to reach out at 080-48655622/33.
              </p>
              <div class="margin-top margin-medium">
                <div
                  blocks-slot-children="ST232"
                  blocks-name="button-group"
                  class="button-group"
                >
                  <a
                    blocks-name="button-2"
                    href="/contact"
                    class="button is-secondary w-button">Contact Us</a
                  >
                </div>
              </div>
            </div>
          </div>
        </header>
        <section class="section_layout350">
          <div class="padding-global-2 background-image">
            <div class="container-large-2">
              <div class="padding-section">
                <div class="w-layout-grid layout350_component">
                  <div
                    id="sticky-tab-container"
                    class="layout350_tabs-menu-2 w-node-_2d790f51-e46b-61e1-7349-fe38e1fd65dd-e424d2c7"
                  >
                    <a
                      href="#Eligibility-criteria"
                      class="layout350_tab-link w-inline-block"
                    >
                      <div>Eligibility criteria</div>
                    </a>
                    <a
                      href="#Admission-procedure"
                      class="layout350_tab-link w-inline-block"
                    >
                      <div>Admission procedure</div>
                    </a>
                    <a
                      href="#Required-Documents"
                      class="layout350_tab-link w-inline-block"
                    >
                      <div>Required Documents</div>
                    </a>
                    <a
                      href="#Fees-structure"
                      class="layout350_tab-link w-inline-block"
                    >
                      <div>Fees structure</div>
                    </a>
                  </div>
                  <div
                    id="w-node-_2d790f51-e46b-61e1-7349-fe38e1fd65dc-e424d2c7"
                    class="layout350_content-wrapper"
                  >
                    <div
                      id="Eligibility-criteria"
                      data-w-id="1795152a-bc8c-5f74-abad-158ef2eec54f"
                      class="layout350_content"
                    >
                      <div class="margin-bottom margin-small">
                        <h2><strong>Eligibility criteria</strong></h2>
                      </div>
                      <div class="section_layout402">
                        <div class="container-large-2">
