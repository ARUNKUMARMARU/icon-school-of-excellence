                            data-current="Tab 2"
                            data-easing="ease"
                            data-duration-in="300"
                            data-duration-out="100"
                            class="layout402_component w-tabs"
                          >
                            <div
                              class="layout402_tabs-menu no-scrollbar w-tab-menu"
                            >
                              <a
                                data-w-tab="Tab 1"
                                class="layout350_tab-link w-inline-block w-tab-link w--current"
                              >
                                <div>NURSERY</div>
                              </a>
                              <a
                                data-w-tab="Tab 2"
                                class="layout350_tab-link w-inline-block w-tab-link"
