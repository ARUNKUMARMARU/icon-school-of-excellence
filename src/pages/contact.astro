      document
        .getElementById('contact-form')
        .addEventListener('submit', function (event) {
          event.preventDefault();

          // Get the submit button and apply loading state
          const submitButton = event.target.querySelector(
            'input[type="submit"]'
          );
          submitButton.classList.add('button-loading');
          submitButton.value = 'Loading...'; // Clear the text

          const formData = new FormData(event.target);
          const data = Object.fromEntries(formData.entries());
          var templateParams = {
            firstname: data.firstname,
            lastname: data.lastname,
            email: data.email,
            phonenumber: data.phonenumber,
            message: data.message,
          };

          emailjs
            .send('service_9tkfj6t', 'template_eu083gf', templateParams)
            .then(function (response) {
              window.alert(
                'Thank you for your interest! We will reach out to you soon.'
              );

              // Reset form and button loading state
              event.target.reset();
              submitButton.classList.remove('button-loading');
              submitButton.value = 'Submit';
            })
            .catch(function (error) {
              console.error('EmailJS Error:', error); // Log error for debugging
              window.alert('Oops! Something went wrong.');

              // Reset button loading state on error
              submitButton.classList.remove('button-loading');
              submitButton.value = 'Submit';
            });
        });
